package br.com.rdi.beans;

public class Order {

	private int id;
	private String date;
	private String customer;
	private double total;
	private boolean toGo;

	public Order() {}
	
	public Order(int id, String date, String customer, double total, boolean toGo) {
		this.id = id;
		this.date = date;
		this.customer = customer;
		this.total = total;
		this.toGo = toGo;
	}
}
